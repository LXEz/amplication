  "version": "0.2.0",
  "configurations": [
    {
      "name": "server",
      "request": "launch",
      "runtimeArgs": ["start"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "cwd": "${workspaceFolder}/to-do-service-server"
    },

    {
      "type": "chrome",
      "request": "launch",
      "name": "admin",
      "url": "http://localhost:3001/",
      "webRoot": "${workspaceFolder}/to-do-service-admin"
    }
  ]
}
